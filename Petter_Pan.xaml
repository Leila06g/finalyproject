<Window x:Class="Finaly_project.Petter_Pan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Finaly_project"
        mc:Ignorable="d"
        Title="Petter_Pan" Height="450" Width="800">


    <Border CornerRadius="10">

        <Border.Background>
            <ImageBrush ImageSource="/Images/Petter_Pan__Read.jpg"
                        Stretch="Fill"/>
        </Border.Background>
        <Border CornerRadius="10"
                BorderThickness="1"
                Opacity="0.95">
        
              
            

            <StackPanel Height="400"  Width="700" Margin="49,16,374,16">

                <TextBlock Text=" PETTER PAN" FontSize="25"  FontFamily="Calibri Bold "  FontWeight="Bold" Height="24"/>
                
                <Button x:Name="Back_Petterbtn" FontSize="20" Content="Back" Margin="-60,15,620,0"  Background="AliceBlue" Width="198" Click="Back_Petterbtn_Click"/>

                <TextBox x:Name="TexBoxPetter" Margin="10,80,350,0"  Height="249" FontFamily="Calibri Bold" FontWeight="Bold"  TextChanged="TextBox_TextChanged" Text=" " />
                
                
            </StackPanel>

        </Border>
    </Border>
</Window>
